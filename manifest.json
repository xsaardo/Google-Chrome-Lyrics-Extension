{
  "manifest_version": 2,

  "name": "Lyrics!",
  "description": "This extension shows a Google Image search result for the current page",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  
  "permissions": [
	"http://genius.com/amp/",
    "activeTab",
	"tabs",
    "https://ajax.googleapis.com/"
  ]
}